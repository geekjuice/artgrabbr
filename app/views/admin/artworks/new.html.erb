<h1>New artwork</h1>
<h2><%= @user.name %> (<%= @user.id %>)</h2>
<div class="row">
	<div class="span8 offset2">
		<div class = "hero-unit">
			<%= form_for([:admin, @artwork]) do |f| %>
				<%= render 'form', f: f %>
				<%= f.submit "Submit", class: "btn btn-large btn-primary" %>
				<%= link_to 'Back', admin_user_path(@user), class:'btn btn-large' %>
			<% end %>
		</div>
	</div>
</div>
