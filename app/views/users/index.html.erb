<h1>Listing users</h1>

<table>
  <tr>
    <th>Email</th>
    <th>First name</th>
    <th>Last name</th>
    <th>School</th>
    <th>Major</th>
    <th>Bio</th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.email %></td>
    <td><%= user.first_name %></td>
    <td><%= user.last_name %></td>
    <td><%= user.school %></td>
    <td><%= user.major %></td>
    <td><%= user.bio %></td>
    <td><%= link_to 'Show', user %></td>
  </tr>
<% end %>
</table>

<br />